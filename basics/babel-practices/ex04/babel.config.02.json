{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "node" : "current"
            }
        }]
    ],
    "plugins": [
        "@babel/plugin-transform-for-of",
        "@babel/plugin-transform-template-literals",
        "@babel/plugin-transform-parameters",
        "@babel/plugin-transform-block-scoping"]
}